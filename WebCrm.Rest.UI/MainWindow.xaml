<Window
    x:Class="WebCrm.Rest.UI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Model="clr-namespace:WebCrm.Rest.Model;assembly=WebCrm.Rest.Model"
    xmlns:local="clr-namespace:WebCrm.Rest.UI"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d"
    Loaded="Window_Loaded"
    Closed="Window_Closed" Initialized="Window_Initialized">
    <Window.Resources>
        <CollectionViewSource x:Key="organisationDtoViewSource" d:DesignSource="{d:DesignInstance {x:Type Model:OrganisationDto}, CreateList=True}"/>
        <CollectionViewSource x:Key="selectedOrganisationDtoViewSource" d:DesignSource="{d:DesignInstance {x:Type Model:OrganisationDto}, CreateList=True}"/>
        <CollectionViewSource x:Key="dupesOrganisationDtoViewSource" d:DesignSource="{d:DesignInstance {x:Type Model:OrganisationDto}, CreateList=True}"/>
        <CollectionViewSource x:Key="personDtoViewSource" d:DesignSource="{d:DesignInstance {x:Type Model:PersonDto}, CreateList=True}"/>
        <CollectionViewSource x:Key="relationDtoViewSource" d:DesignSource="{d:DesignInstance {x:Type Model:RelationDto}, CreateList=True}"/>
    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="6"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition />
            </Grid.RowDefinitions>

            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Label Content="Filter" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}" />
                <TextBox x:Name="FilterTextBox" Grid.Column="1" TextChanged="FilterTextBox_TextChanged" KeyUp="FilterTextBox_KeyUp" />
            </Grid>

            <TabControl Grid.Row="1" SelectionChanged="TabControl_SelectionChanged">

                <TabItem Header="Organisations" >

                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="6"/>
                            <ColumnDefinition Width="3*" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <TabControl>
                                <TabItem Header="All Organisations" x:Name="OrganisationsTabItem" >
                                    <Grid DataContext="{StaticResource organisationDtoViewSource}">
                                        <DataGrid x:Name="organisationDtoDataGrid" AutoGenerateColumns="False" EnableRowVirtualization="True" ItemsSource="{Binding}" RowDetailsVisibilityMode="VisibleWhenSelected" SelectionChanged="BaseObjectDataGrid_SelectionChanged">
                                            <DataGrid.Columns>
                                                <DataGridTextColumn x:Name="organisationNameColumn" Binding="{Binding OrganisationName}" Header="Name" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationDivisionNameColumn" Binding="{Binding OrganisationDivisionName}" Header="Division Name" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationExtraCustom1Column" Binding="{Binding OrganisationExtraCustom1}" Header="Extra Custom 1" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationExtraCustom2Column" Binding="{Binding OrganisationExtraCustom2}" Header="Extra Custom 2" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationVatNumber" Binding="{Binding OrganisationVatNumber}" Header="VAT Number" Width="SizeToHeader"/>
                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </Grid>
                                </TabItem>
                                <TabItem Header="Duplicate CVR Numbers" x:Name="DupeOrganisationsTabItem" >
                                    <Grid DataContext="{StaticResource dupesOrganisationDtoViewSource}">
                                        <DataGrid x:Name="dupeOrganisationDtoDataGrid" AutoGenerateColumns="False" EnableRowVirtualization="True" ItemsSource="{Binding}" RowDetailsVisibilityMode="VisibleWhenSelected" SelectionChanged="BaseObjectDataGrid_SelectionChanged">
                                            <DataGrid.Columns>
                                                <DataGridTextColumn x:Name="organisationNameColumn2" Binding="{Binding OrganisationName}" Header="Name" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationDivisionNameColumn2" Binding="{Binding OrganisationDivisionName}" Header="Division Name" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationExtraCustom1Column2" Binding="{Binding OrganisationExtraCustom1}" Header="Extra Custom 1" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationExtraCustom2Column2" Binding="{Binding OrganisationExtraCustom2}" Header="Extra Custom 2" Width="SizeToHeader"/>
                                                <DataGridTextColumn x:Name="organisationVatNumber2" Binding="{Binding OrganisationVatNumber}" Header="VAT Number" Width="SizeToHeader"/>
                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </Grid>
                                </TabItem>
                            </TabControl>
                        </Grid>
                        <GridSplitter Grid.Column="1" Background="BlanchedAlmond" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
                        <ScrollViewer Grid.Column="2" VerticalScrollBarVisibility="Auto">
